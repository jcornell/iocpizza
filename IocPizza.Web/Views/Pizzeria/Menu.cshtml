<div id="body">
    <section class="featured">
        <h2>Menu</h2>
    </section>
    <h3>@ViewBag.Error</h3>
    <ul>
            @foreach (var menuItem in @ViewBag.MenuItems)
            {
                <li><h3>@menuItem.Title.....From $@menuItem.Price</h3></li>
            }
        </ul>
</div>

